<%= form_for(@article) do |f| %>
  <%= error_messages(@article) %>
  <div class="field form-group">
    <%= f.label :title %><br>
    <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class="field form-group">
    <%= f.label :body %><br>
    <%= f.text_area :body,rows: 10, class: 'form-control'  %>
  </div>
  <div class="actions form-group">
    <%= f.submit %>
  </div>
<% end %>
